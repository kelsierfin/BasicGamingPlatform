+package ca.ucalgary.seng.p3;
+
+import javafx.fxml.FXML;
+import javafx.fxml.FXMLLoader;
+import javafx.scene.Scene;
+import javafx.scene.layout.AnchorPane;
+
+import java.io.IOException;
+
+public class PageNavigator {
+    public static Scene navigateTo(String pageName) throws IOException {
+        String fxmlPath = "/" + pageName + ".fxml";
+
+        System.out.println("Loading: " + fxmlPath);
+
+        FXMLLoader loader = new FXMLLoader(PageNavigator.class.getResource(fxmlPath));
+
+        return new Scene(loader.load(), 900, 600);
+    }
+}
